@model IEnumerable<RahalWeb.Models.DeffEmpTreatment>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DeffCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeffName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeleteFlag)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DeffCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeffName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeleteFlag)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
